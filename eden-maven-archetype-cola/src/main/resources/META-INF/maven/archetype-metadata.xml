<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor
	xsi:schemaLocation="https://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0 http://maven.apache.org/xsd/archetype-descriptor-1.1.0.xsd"
	xmlns="https://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<fileSets>
		<fileSet filtered="true" encoding="UTF-8">
			<directory>src/sql</directory>
			<includes>
				<include>**/*.sql</include>
				<include>**/*.pdm</include>
				<include>**/*.pdb</include>
			</includes>
		</fileSet>

		<fileSet encoding="UTF-8">
			<directory>src/docs</directory>
			<includes>
				<include>**/*.svg</include>
				<include>**/*.puml</include>
			</includes>
		</fileSet>

		<fileSet filtered="true" encoding="UTF-8">
			<directory></directory>
			<includes>
				<include>.gitlab-ci.yml</include>
				<include>.editorconfig</include>
				<include>.gitignore</include>
				<include>build.bat</include>
				<include>README.md</include>
				<include>CHANGELOG.md</include>
				<include>CONTRIBUTING.md</include>
			</includes>
		</fileSet>
	</fileSets>

	<requiredProperties>
		<requiredProperty key="author">
			<defaultValue>gyl</defaultValue>
		</requiredProperty>
		<requiredProperty key="email">
			<defaultValue>1813986321@qq.com</defaultValue>
		</requiredProperty>
		<requiredProperty key="description">
			<defaultValue>DDD架构</defaultValue>
		</requiredProperty>
	</requiredProperties>

</archetype-descriptor>
